{
  "name": "xblade-apii",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "dev:clear": "rimraf node_modules/.cache && nodemon app.js",
    "dev:verbose": "nodemon --verbose app.js",
    "dev:inspect": "nodemon --inspect app.js",
    "restart": "npm run kill && npm run dev",
    "kill": "node -e \"try { process.platform === 'win32' ? require('child_process').execSync('taskkill /f /im node.exe 2>nul', {stdio: 'ignore'}) : require('child_process').execSync('pkill -f \\\"node.*app.js\\\" 2>/dev/null', {stdio: 'ignore'}); } catch(e) { console.log('No processes to kill'); }\"",
    "clean": "rimraf node_modules/.cache && rimraf dist",
    "reset": "npm run clean && npm install"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "express": "~4.16.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "eslint": "^9.33.0",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "rimraf": "^5.0.5"
  }
}
